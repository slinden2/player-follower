{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "lodash": "^4.17.19",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook-pixel": "^1.0.3",
    "react-ga": "^3.1.2",
    "react-media": "^1.9.2",
    "react-remove-scroll": "^2.3.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "reaptcha": "^1.7.2",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.9.0",
    "unfetch": "^4.1.0",
    "yup": "^0.29.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:ci": "cd ../backend && npm run start:ci & npm start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_$"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "chai-colors": "^1.0.1",
    "cypress": "^4.10.0",
    "eslint-plugin-cypress": "^2.11.1"
  }
